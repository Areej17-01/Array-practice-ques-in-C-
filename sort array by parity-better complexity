int main() {
    int array[7] = { 1,2,3,4,3,2,1 };
    int k = 0;
    int f = 6;
    int arr[7];
    for (int i = 0; i < 7; i++) {
        if (array[i] % 2 == 0) {
            arr[k] = array[i];
            k++;
        }
        else if (array[i] % 2 != 0) {
            arr[f] = array[i];
            f=f-1;

        }
        

    }
    print(arr, 7);
}
