
int main()
{
    //int array[2][2] = { {2,3},{3,4} };
    int col = 2;
    int row = 3;
    int arr[3][2] = { {5,6},{3.4},{8,0} };
    for (int i = 0; i < row; i++) {
        for (int j = 0; j < col; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    cout << endl;
    for (int i = 0; i < col; i++) {
        for (int j = 0; j < row; j++) {
            cout << arr[j][i] << " ";
        }
        cout << endl;
    }

}
