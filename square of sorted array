void print(int array[], int n) {
    for (int i = 0; i < n; i++) {
        cout << array[i] << ",";
    }
    return;
}
int main()
{
    int n;
    cin >> n;
    int* array{ new int[n] };
    int k;
    for (int i = 0; i < n; i++) {
        cin >> k;
        array[i] = k*k;
    }
    //function for printing nums
    print(array, n);

}
